<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget28"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
	<TableLayout android:id="@+id/widget31"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical">
	</TableLayout>
	<TextView android:id="@+id/tx_help" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Ingresa la placa"
		android:gravity="center">
	</TextView>
	<EditText android:id="@+id/et_carplate" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:textSize="18sp"
		android:gravity="center">
	</EditText>
	<Button android:id="@+id/bt_query" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="Consultar"
		android:textStyle="bold">
	</Button>
	<RatingBar android:id="@+id/ratingBar" android:layout_width="wrap_content" android:layout_gravity="center" android:visibility="gone"
    android:layout_height="wrap_content"></RatingBar>
	<TextView android:id="@+id/tx_output" android:layout_width="fill_parent"
		android:layout_height="wrap_content">
	</TextView>
</LinearLayout>